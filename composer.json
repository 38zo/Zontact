{
    "name": "38zo/zontact",
    "description": "One button, one form, zero hassle.",
    "minimum-stability": "stable",
    "type": "wordpress-plugin",
    "license": "GPL-3.0",
    "authors": [
    {
        "name": "38zo",
        "email": "frenziecodes@gmail.com"
    }
    ],
    "autoload": {
    "psr-4": {
        "38zo\\Zontact\\": "includes/"
    },
    "classmap": [
        "includes/"
    ]
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
        }
    },
    "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "wp-coding-standards/wpcs": "2.*"
    },
    "config": {
    "allow-plugins": {
        "composer/installers": true
    }
    },
    "require": {
        "composer/installers": "^1.12"
    }
}